---- Arithmetic
red appCPO( [[ 1 / 0 ]], x |-> 2 ) .
red appCPO( [[ 1 / 0 + 0 ]], x |-> 2 ) .
red appCPO( [[ 1  + 1 / 0 ]], x |-> 2 ) .
red appCPO( [[ 1 / 0 + 1 / 0 ]], x |-> 2 ) .

red appCPO( [[ 1 / 0 ]], x |-> 2 ) .
red appCPO( [[ ( 1 / 0 ) / 1 ]], x |-> 2 ) .
red appCPO( [[ 1 / ( 1 / 0 ) ]], x |-> 2 ) .
red appCPO( [[ ( 1 / 0 ) / ( 1 / 0 ) ]], x |-> 2 ) .
red appCPO( [[ 1 + 2 + 3 + ( 1 + 1 / 0 + 1) / ( 1 / 0 ) + 1 ]], x |-> 2 ) .
red appCPO( [[ 1 + 2 + 3 + ( 1 + 1 / 1 + 1) / ( 1 / 1 ) + 1 ]], x |-> 2 ) .
red appCPO( [[ 1 + 2 + x + ( x + 1 / 0 + 1) / ( x / 0 ) + 1 ]], x |-> 2 ) .

---- Boolean
red appCPO( [[ 1 <= 2 ]], x |-> 2 ) .
red appCPO( [[ x <= 1 ]], x |-> 2 ) .
red appCPO( [[ x <= 1 ]], x |-> 2 ) .
red appCPO( [[ x / 0 <= 1 ]], x |-> 2 ) .
red appCPO( [[ x <= 1 / 0 ]], x |-> 2 ) .
red appCPO( [[ x / 0 <= 1 / 0 ]], x |-> 2 ) .

red appCPO( [[! x <= 1 ]], x |-> 2) .
red appCPO( [[! 1 <= 2 ]], x |-> 2) .
red appCPO( [[! 1 / 0 <= 2 ]], x |-> 2) .
red appCPO( [[! 1  <= 2  / 0 ]], x |-> 2) .
red appCPO( [[! 1 / 0  <= 2  / 0 ]], x |-> 2) .

red appCPO( [[ true && true ]], x |-> 2 ) .
red appCPO( [[ true && false ]], x |-> 2 ) .
red appCPO( [[ false && true ]], x |-> 2 ) .
red appCPO( [[ false && false ]], x |-> 2 ) .
red appCPO( [[ 1 / 0  <= 2 && false ]], x |-> 2 ) .
red appCPO( [[ 1 / 0  <= 2 && true ]], x |-> 2 ) .
red appCPO( [[ true && 1 / 0  <= 2 ]], x |-> 2 ) .
red appCPO( [[ false  && 1 / 0  <= 2 ]], x |-> 2 ) .

q
